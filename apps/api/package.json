    "nanoid": "^5.0.0",
    "postgres": "^3.4.0",
    "socket.io": "^4.7.0",
    "stockfish": "^16.0.0",
    "viem": "^2.9.0",
    "ws": "^8.16.0",
    "zod": "^3.22.0"
  },
